{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "cache": true
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "bot#dev": {
      "env": ["DISCORD_BOT_TOKEN"]
    },
    "api#dev": {
      "env": [
        "DISCORD_BOT_TOKEN",
        "DISCORD_CLIENT_ID",
        "DISCORD_CLIENT_SECRET",
        "DISCORD_CALLBACK_URL",
        "JWT_SECRET"
      ]
    }
  },
  "ui": "tui"
}
